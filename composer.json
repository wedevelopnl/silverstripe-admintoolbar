{
    "name": "wedevelopnl/silverstripe-admintoolbar",
    "description": "Admin toolbar for silverstripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "admin",
        "toolbar",
        "frontend"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "WeDevelop",
        "email": "development@wedevelop.nl"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "silverstripe/cms": "^5.0@stable"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.20",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/extension-installer": "^1.0",
        "syntro/silverstripe-phpstan": "^5.0",
        "wernerkrauss/silverstripe-rector": "dev-main"
    },
    "extra": {
        "installer-name": "silverstripe-admintoolbar",
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "WeDevelop\\AdminToolbar\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
